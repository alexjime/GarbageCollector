    <servlet>
        <servlet-name>checkLogin</servlet-name>
        <servlet-class>helper.LoginServlet</servlet-class>
    </servlet>
  
  <servlet-mapping>
	<servlet-name>mvc_do</servlet-name>
	<url-pattern>*.do</url-pattern>
